num=int(input())

if num>=1000 and num<=9999:
    a = num // 1000
    num=num%1000
    b = num // 100
    num=num%100
    c = num // 10
    num=num%10
    d = num // 1


    if (a+b+c+d)%2==0:
        print("É número paridário")
    else:
        print("Não é número paridário")
else:
    print("ERRO")
